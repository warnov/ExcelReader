<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <!-- The identifier that must be unique within the hosting gallery -->
        <id>WarNov.ExcelReader</id>
		<title>WarNov.ExcelReader</title>
		<summary>This is a lightweight Excel Reader using DocumentFormat.OpenXml. It just does one thing: Read the content of a cell and return it as a string.</summary>

        <!-- The package version number that is used when resolving dependencies -->
        <version>2.0</version>

        <!-- Authors contain text that appears directly on the gallery -->
        <authors>warnov</authors>

        <!-- 
            Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  
        -->
        <owners>warnov</owners>

         <!-- License and project URLs provide links for the gallery -->
        <licenseUrl>http://opensource.org/licenses/MS-PL</licenseUrl>
        

        <!-- The icon is used in Visual Studio's package manager UI -->
        <iconUrl>https://sacredialoanzanotifier.blob.core.windows.net/waresources/excelicon.png</iconUrl>

        <!-- 
            If true, this value prompts the user to accept the license when
            installing the package. 
        -->
        <requireLicenseAcceptance>false</requireLicenseAcceptance>

        <!-- Any details about this particular release -->
        <releaseNotes>With these new release, columns are not required to be passed by number. Just pass the name of the column as a string: "AB". This makes more intuitive the use of the library but above all solve some issues when reading empty cells.</releaseNotes>

        <!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
        <description>Excel Reader using DocumentFormat.OpenXml. Reads the content of a cell and returns it as a string.</description>

        <!-- Copyright information -->
        <copyright>Copyright Â©2018 WarNov</copyright>

        <!-- Tags appear in the gallery and can be used for tag searches -->
        <tags>excel read utility OpenXML cell library</tags>
		<projectUrl>https://github.com/warnov/ExcelReader</projectUrl>
		<repository type="git" url="https://github.com/warnov/ExcelReader" />

        <!-- Dependencies are automatically installed when the package is installed -->
        <dependencies>
            <dependency id="DocumentFormat.OpenXml" version="2.8.1" />
        </dependencies>
		<language>English</language>
    </metadata>
</package>